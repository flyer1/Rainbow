{
    "src": {
        "js": {
            "all": [ "src/client/**/*.js", "!src/client/lib/**" ]
        },
        "css": [ "src/css/app.css" ],
        "vendorCss": [ "src/lib/ionic/release/css/ionic.css", "src/lib/angular-toastr/dist/angular-toastr.css" ],
        "fonts": [ "src/client/fonts/*" ],
        "images": [ "src/client/img/*", "!src/client/img/resources" ],
        "indexHtml": [ "src/client/index2.html" ],
        "templates": [ "src/**/*.html", "!src/client/index.html", "!src/lib/**" ]
    },
    "dist": {
        "root": "./dist/",
        "js": "./dist/js/",
        "css": "./dist/css/",
        "fonts": "./dist/fonts/",
        "images": "./dist/img/"
    }
}